generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Supplier {
  id      Int        @id @default(autoincrement())
  name    String
  phone   String
  address String
  product Products[]
}

model Products {
  id          Int      @id @default(autoincrement())
  description String
  priceSales  Decimal
  supplier    Supplier @relation(fields: [supplierId], references: [id])
  supplierId  Int
}
